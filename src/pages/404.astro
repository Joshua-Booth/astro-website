---
import BaseLayout from "../layouts/BaseLayout.astro"
---

<BaseLayout title="Not Found">
  <section class="main text-center section mb-3">
    <img
      src="/assets/images/404_page_not_found.svg"
      class="mb-2 mh-50 w-75"
      alt="404 Image"
    />
    <h2>Page Not Found!</h2>
    <p class="col-10 text-center mx-auto">
      This page was not found, you can go back and search for the page.
    </p>
  </section>
</BaseLayout>